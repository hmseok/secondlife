{
  "name": "SelfDisruption",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "lint": "eslint .",
    "clean": "cd android && ./gradlew clean && cd .. && cd ios && xcodebuild clean && cd ..",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace SelfDisruption.xcworkspace -scheme SelfDisruption -configuration Release"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-native": "0.76.6",

    "@react-navigation/native": "^7.0.0",
    "@react-navigation/native-stack": "^7.0.0",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "react-native-screens": "^4.4.0",
    "react-native-safe-area-context": "^4.12.0",
    "react-native-gesture-handler": "^2.20.0",
    "react-native-reanimated": "^3.16.0",

    "@supabase/supabase-js": "^2.39.0",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "react-native-url-polyfill": "^2.0.0",

    "react-native-vector-icons": "^10.2.0",

    "react-native-vision-camera": "^4.6.0",
    "react-native-image-picker": "^7.1.0",
    "@react-native-community/geolocation": "^3.4.0",
    "@react-native-firebase/app": "^21.0.0",
    "@react-native-firebase/messaging": "^21.0.0",
    "@notifee/react-native": "^9.1.0",
    "react-native-permissions": "^4.1.0",

    "react-native-keychain": "^9.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "@types/react-native-vector-icons": "^6.4.0",
    "typescript": "~5.3.0",
    "@react-native/babel-preset": "^0.76.0",
    "@react-native/eslint-config": "^0.76.0",
    "@react-native/metro-config": "^0.76.0",
    "@react-native/typescript-config": "^0.76.0"
  }
}
